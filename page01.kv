WindowManager:
	FirstWindow:
	SecondWindow:
	ThirdWindow:
	FourthWindow:
	FifthWindow:


<FirstWindow>:
    name: 'first'
    Screen:
        Image:
            source: "C:/Users/RUDAINA/Desktop/sh/background.jpg"
            allow_stretch: True
            keep_ratio: False

        MDCard:
            size_hint: (0.8, 0.8)
            pos_hint: {"center_x": 0.5, "center_y": 0.5}
            #elevation: 10
            padding: 15
            spacing: 10
            orientation: 'vertical'

            Image:
                source: "C:/Users/RUDAINA/Desktop/sh/logo.png"
                pos_hint: {"center_x": 0.5, "center_y": 0.5}

            MDLabel:
                id: welcome_label
                text: "WELCOME TO QUICKBITE"
                font_size: 25
                halign: 'center'
                size_hint_y: 0.5
                #height: self.texture_size[1]
                padding_y: 10

            MDLabel:
                id: name_label
                text: "Please Enter Your Name"
                font_size: 15
                halign: 'center'
                size_hint_y: 0.5

                #height: self.texture_size[1]
                padding_y: 10

            MDTextField:
                id: name
                hint_text: "name"
                size_hint_x: 0.5
                font_size: 18
                pos_hint: {"center_x": 0.5}


            MDRoundFlatButton:
                text: "TAKE A QUICK BITE"
                font_size: 12
                pos_hint: {"center_x": 0.5}
                on_press: app.logger()

            Widget:
                size_hint_y: 0.5
